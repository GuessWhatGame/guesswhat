{
  "inputs": {
    "question": true,
    "category": true,
    "spatial": true,
    "crop": false,
    "image": false
  },

  "model": {
    "question": {
      "no_LSTM_hiddens": 512,
      "embedding_dim": 300
    },

    "category": {
      "n_categories": 90,
      "embedding_dim": 512
    },

    "MLP": {
      "num_hiddens": 512
    },

    "image": {
      "image_input": "features",
      "dim": [1000]
    },

    "crop": {
      "image_input": "features",
      "dim": [1000],
      "scale" : 1.1
    }

  },

  "optimizer": {
    "no_epoch": 20,
    "learning_rate": 1e-4,
    "batch_size": 64,
    "clip_val": 5
  },

  "status": ["success"],
  "seed": -1

}


